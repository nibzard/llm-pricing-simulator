{
  "id": "jtbd-3-comparison",
  "name": "JTBD 3: Compare LLM Framing (High Usage)",
  "models": [
    "claude-opus-4",
    "claude-sonnet-4.5",
    "gpt-5",
    "gpt-5-mini",
    "gemini-2.5-pro"
  ],
  "intent_groups": [
    {
      "name": "Multi-model comparison analysis",
      "intents_count": 80,
      "variants_per_intent": 4,
      "frequency": "2_hourly",
      "flow_steps": [
        {
          "name": "main-answer",
          "uses_model": "current",
          "input_tokens_strategy": "fixed",
          "fixed_input_tokens": 200,
          "expected_output_tokens": 600,
          "runs_per_prompt": 1
        },
        {
          "name": "judge-differences",
          "uses_model": "gpt-5-mini",
          "input_tokens_strategy": "fixed",
          "fixed_input_tokens": 3000,
          "expected_output_tokens": 300,
          "runs_per_prompt": 1,
          "comment": "One judge model compares all 5 model outputs"
        }
      ]
    }
  ],
  "days_per_month": 30
}
