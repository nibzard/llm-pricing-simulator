{
  "id": "jtbd-1-brand-category",
  "name": "JTBD 1: Brand/Category Monitoring (Daily)",
  "models": [
    "claude-sonnet-4.5",
    "gpt-5",
    "gemini-2.5-flash"
  ],
  "intent_groups": [
    {
      "name": "Brand and category monitoring",
      "intents_count": 30,
      "variants_per_intent": 3,
      "frequency": "daily",
      "flow_steps": [
        {
          "name": "main-answer",
          "uses_model": "current",
          "input_tokens_strategy": "fixed",
          "fixed_input_tokens": 150,
          "expected_output_tokens": 500,
          "runs_per_prompt": 1
        },
        {
          "name": "extract-entities",
          "uses_model": "gpt-5-mini",
          "input_tokens_strategy": "from_previous_output",
          "expected_output_tokens": 200,
          "runs_per_prompt": 1
        }
      ]
    }
  ],
  "days_per_month": 30
}
