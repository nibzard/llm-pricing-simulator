{
  "id": "jtbd-4-competitors",
  "name": "JTBD 4: Competitor Discovery (Moderate Usage)",
  "models": [
    "claude-sonnet-4.5",
    "gpt-5",
    "gemini-2.5-pro"
  ],
  "intent_groups": [
    {
      "name": "Competitor mentions in evaluative queries",
      "intents_count": 40,
      "variants_per_intent": 4,
      "frequency": "daily",
      "flow_steps": [
        {
          "name": "main-answer",
          "uses_model": "current",
          "input_tokens_strategy": "fixed",
          "fixed_input_tokens": 180,
          "expected_output_tokens": 700,
          "runs_per_prompt": 1
        },
        {
          "name": "extract-entities",
          "uses_model": "claude-4.5-haiku",
          "input_tokens_strategy": "from_previous_output",
          "expected_output_tokens": 300,
          "runs_per_prompt": 1,
          "comment": "Haiku extracts competitors from all 3 model outputs"
        },
        {
          "name": "competitor-cluster",
          "uses_model": "gpt-5-mini",
          "input_tokens_strategy": "fixed",
          "fixed_input_tokens": 1200,
          "expected_output_tokens": 150,
          "runs_per_prompt": 1,
          "comment": "Mini groups competitors extracted from all 3 models"
        }
      ]
    }
  ],
  "days_per_month": 30
}
